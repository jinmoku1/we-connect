<%- include partials/header.ejs %>

<h1><%=title%></h1>

<h3>Developers</h3>

<div class="row">
	<% for (var i in developer) { %>
		<div class="col-sm-6 col-md-4">
			<div class="thumbnail">
				<img height="300" width="200" alt="..." src="<%=developer[i].pic%>">
				<div class="caption">
					<h3><%=developer[i].name%></h3>
					<p><%=developer[i].info%></p>
				</div>
			</div>
		</div>
	<% } %>
</div>

<%- include partials/footer.ejs %>